<%@ Page Title="" Language="C#" MasterPageFile="~/MainMaster.Master" AutoEventWireup="true" CodeBehind="booking.aspx.cs" Inherits="BAHRAIN_TOURS.booking" %>
<asp:Content ID="Content1" ContentPlaceHolderID="head" runat="server">
</asp:Content>
<asp:Content ID="Content2" ContentPlaceHolderID="ContentPlaceHolder1" runat="server">
        <div class="container">        <h1 class="text-center" style="color: green">Booking Page</h1>        <asp:Label ID="lblSession" runat="server" Text=""></asp:Label>        <div class="row">            <div class="col-md-8">                <div class="row txtrow">                    <div class="col-md-3">                        Id                    </div>                    <div class="col-md-9">                        <asp:TextBox class="resizedTextbox" ID="txtID" runat="server" BackColor="#efefef" ReadOnly="true"></asp:TextBox>                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                        Tour Name                    </div>                    <div class="col-md-9">                        <asp:TextBox BackColor="#efefef" ReadOnly="true" class="resizedTextbox" ID="txtTourName" runat="server"></asp:TextBox>                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                        <lable>Package Type :</lable>                    </div>                    <div class="col-md-9">                        <asp:DropDownList class="resizedTextbox" ID="ddlPackageType" runat="server">                            <asp:ListItem Text="Please choose package" Value=""></asp:ListItem>                            <asp:ListItem Text="Basic" Value="Basic"></asp:ListItem>                            <asp:ListItem Text="Gold" Value="Gold"></asp:ListItem>                            <asp:ListItem Text="Premium" Value="Premium"></asp:ListItem>                        </asp:DropDownList>                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                        <lable>Payment Type :</lable>                    </div>                    <div class="col-md-9">                        <asp:DropDownList class="resizedTextbox" ID="ddlPaymentType" runat="server">                            <asp:ListItem Text="Please choose Payment Type" Value=""></asp:ListItem>                            <asp:ListItem Text="Cash" Value="Cash"></asp:ListItem>                            <asp:ListItem Text="Card" Value="Card"></asp:ListItem>                        </asp:DropDownList>                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                        <lable>Decription:</lable>                    </div>                    <div class="col-md-9">                        <textarea class="resizedTextbox" id="txtDesc" cols="20" rows="4" runat="server"></textarea>                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                        <lable>Start Date:</lable>                    </div>                    <div class="col-md-9">                        <input class="resizedTextbox" id="StartDate" type="date" runat="server" />                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                        <lable>End Date:</lable>                    </div>                    <div class="col-md-9">                        <input class="resizedTextbox" id="EndDate" type="date" runat="server" />                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                        <lable>Price:</lable>                    </div>                    <div class="col-md-9">                        <asp:TextBox class="resizedTextbox" ReadOnly="true" BackColor="#efefef" ID="txtPrice" runat="server" Text=""></asp:TextBox>                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                                           </div>                    <div class="col-md-9">                        <asp:TextBox class="resizedTextbox" type="hidden" ID="txtCompanyID" runat="server" Text="">1</asp:TextBox>                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                                           </div>                    <div class="col-md-9">                        <asp:TextBox class="resizedTextbox" type="hidden" ID="txtGuideID" runat="server" Text="">1</asp:TextBox>                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                    </div>                    <div class="col-md-9">                        <asp:TextBox class="resizedTextbox" BackColor="#efefef" ID="txtUsername" runat="server" Text="" type="hidden" ReadOnly="true"></asp:TextBox>                    </div>                </div>                <div calss="row txtrow">                    <div class="col-md-3">                    </div>                    <div class="col-md-9" style="align-items: flex-start;">                        <asp:Button ID="btnBooking" runat="server" Text="Booking a trip" class="btn btn-success" OnClick="btnBooking_Click" />                        <asp:Button ID="btnCancel" runat="server" Text="Cancel" class="btn btn-danger" OnClick="btnCancel_Click" />                    </div>                </div>            </div>            <div class="col-md-4">            </div>        </div>    </div>
</asp:Content>

<%@ Page Title="Contact Us" Language="C#" MasterPageFile="~/MainMaster.Master" AutoEventWireup="true" CodeBehind="Contact.aspx.cs" Inherits="BAHRAIN_TOURS.Contact" %>
<asp:Content ID="Content1" ContentPlaceHolderID="head" runat="server">
</asp:Content>
<asp:Content ID="Content2" ContentPlaceHolderID="ContentPlaceHolder1" runat="server">
<div class="container">    <h1 runat="server" id="divH1" class="message">Contact Us</h1>    <div class="row">        <div class="col-md-8">            <div class="row txtrow">                <div class="col-md-3">                    <lable>Name :</lable>                </div>                <div class="col-md-9">                    <asp:TextBox ID="txtName" CssClass="txt-box" runat="server"></asp:TextBox>                </div>            </div>            <div class="row txtrow">                <div class="col-md-3">                    <lable>Email :</lable>                </div>                <div class="col-md-9">                    <asp:TextBox ID="txtEmail" runat="server"></asp:TextBox>                </div>            </div>            <div class="row txtrow">                <div class="col-md-3">                    <lable>Age :</lable>                </div>                <div class="col-md-9">                    <asp:TextBox ID="txtAge" runat="server"></asp:TextBox>                </div>            </div>            <div class="row txtrow">                <div class="col-md-3">                    <lable>Notes/Problem :</lable>                </div>                <div class="col-md-9">                    <textarea id="txtMsg" cols="20" rows="4" runat="server"></textarea>                </div>            </div>            <div class="row txtrow">                <div class="col-md-3">                    <lable>Your Package :</lable>                </div>                <div class="col-md-9">                    <asp:DropDownList ID="ddlPackage" runat="server">                        <asp:ListItem Text="Please choose a Package" Value=""></asp:ListItem>                        <asp:ListItem Text="Package 1" Value="Package_1"></asp:ListItem>                        <asp:ListItem Text="Package 2" Value="Package_2"></asp:ListItem>                        <asp:ListItem Text="Package 3" Value="Package_3"></asp:ListItem>                    </asp:DropDownList>                </div>            </div>            <div class="row txtrow">                <div class="col-md-3">                </div>                <div class="col-md-9">                    <asp:Button class="btn btn-success" ID="btnSubmit" runat="server" Text="Submit Info" OnClick="btnSubmit_Click" />                </div>            </div>            <div class="row txtrow">                <div class="col-md-9">                    <div class="alert-info ">                        <div id="divMessage" runat="server" class="divMessage">                            <asp:Literal runat="server" ID="ltMessage"></asp:Literal>                        </div>                    </div>                </div>                <div class="col-md-3">                </div>            </div>        </div>    </div>    <div class="col-md-4">    </div></div>
</asp:Content>

<%@ Page Title="" Language="C#" MasterPageFile="~/MainMaster.Master" AutoEventWireup="true" CodeBehind="singup_company.aspx.cs" Inherits="BAHRAIN_TOURS.singup_company" %>
<asp:Content ID="Content1" ContentPlaceHolderID="head" runat="server">
</asp:Content>
<asp:Content ID="Content2" ContentPlaceHolderID="ContentPlaceHolder1" runat="server">
                      <div class="container" style="margin-top: 100px;">        <div class="col-md-9">            <div class="row">                <div class="row">                    <div class="col-md-3">                    </div>                    <div class="col-md-9">                        <h1>Registeration (Comapny)</h1>                    </div>                </div>                <div class="row">                    <div class="col-md-3">                    </div>                    <div class="col-md-9">                        <asp:ValidationSummary ID="ValidationSummary1" runat="server" Font-Bold="False" ForeColor="Red" ValidationGroup="signup" />                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                    </div>                    <div class="col-md-9">                        <asp:TextBox placeholder="Type your CompanyName" class="resizedTextbox" ID="txtCompanyName" runat="server"></asp:TextBox>                        <asp:RegularExpressionValidator ID="RegularExpressionValidator4" runat="server" ControlToValidate="txtCompanyName" ErrorMessage="Minimum 3 and Maximum 8 characters required for username." Font-Bold="True" ForeColor="#FF3300" ValidationExpression="^[\s\S]{3,8}$" ValidationGroup="signup">*</asp:RegularExpressionValidator>                        <asp:RequiredFieldValidator ID="RequiredFieldValidator1" runat="server" ControlToValidate="txtCompanyName" ErrorMessage="Company Name is required" Font-Bold="True" ForeColor="#FF3300" ValidationGroup="signup">*</asp:RequiredFieldValidator>                        <asp:RegularExpressionValidator ID="RegularExpressionValidator3" runat="server" ControlToValidate="txtCompanyName" ErrorMessage="Special chars are not allowed for company name" Font-Bold="True" ForeColor="#FF3300" ValidationExpression="^[a-zA-Z0-9,.# ]*$" ValidationGroup="signup">*</asp:RegularExpressionValidator>                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                    </div>                    <div class="col-md-9">                        <asp:TextBox placeholder="Type your Password" class="resizedTextbox" ID="txtPassword" runat="server" TextMode="Password"></asp:TextBox>                        <asp:RegularExpressionValidator ID="RegularExpressionValidator5" runat="server" ControlToValidate="txtPassword" ErrorMessage="Minimum 3 and Maximum 15 characters required for password." Font-Bold="True" ForeColor="#FF3300" ValidationExpression="^[\s\S]{3,15}$" ValidationGroup="signup">*</asp:RegularExpressionValidator>                        <asp:RequiredFieldValidator ID="RequiredFieldValidator2" runat="server" ControlToValidate="txtPassword" ErrorMessage="Password is required" Font-Bold="True" ForeColor="#FF3300" ValidationGroup="signup">*</asp:RequiredFieldValidator>                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                    </div>                    <div class="col-md-9">                        <asp:TextBox placeholder="Type your Confirm Password" class="resizedTextbox" ID="txtConfirmPassword" runat="server" TextMode="Password"></asp:TextBox>                        <asp:RequiredFieldValidator ID="RequiredFieldValidator3" runat="server" ControlToValidate="txtConfirmPassword" ErrorMessage="Confirm password is required" Font-Bold="True" ForeColor="#FF3300" ValidationGroup="signup">*</asp:RequiredFieldValidator>                        <asp:CompareValidator ID="CompareValidator1" runat="server" ControlToCompare="txtPassword" ControlToValidate="txtConfirmPassword" ErrorMessage="Password does not match" Font-Bold="True" ForeColor="#009933" ValidationGroup="signup">*</asp:CompareValidator>                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                    </div>                    <div class="col-md-9">                        <asp:TextBox placeholder="Type your Company Phone" class="resizedTextbox" ID="txtPhone" runat="server"></asp:TextBox>                        <asp:RegularExpressionValidator ID="RegularExpressionValidator2" runat="server" ControlToValidate="txtPhone" ErrorMessage="Phone is not a bahraini number" Font-Bold="True" ForeColor="#FF3300" ValidationExpression="[0-9]{8}" ValidationGroup="signup">*</asp:RegularExpressionValidator>                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                    </div>                    <div class="col-md-9">                        <asp:TextBox placeholder="Type your Company Email" class="resizedTextbox" ID="txtEmail" runat="server"></asp:TextBox>                        <asp:RegularExpressionValidator ID="RegularExpressionValidator1" runat="server" ControlToValidate="txtEmail" ErrorMessage="Email is not valid" Font-Bold="True" ForeColor="#FF3300" ValidationExpression="\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*" ValidationGroup="signup">*</asp:RegularExpressionValidator>                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                    </div>                    <div class="col-md-9">                        <asp:TextBox ID="txtApproved" runat="server" type="hidden" Text="0"></asp:TextBox>                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                    </div>                    <div class="col-md-9">                        <textarea placeholder="Type your Desc" class="resizedTextbox" id="txtDesc" cols="20" rows="4" runat="server"></textarea>                    </div>                </div>                <div calss="row txtrow">                    <div class="col-md-3">                    </div>                    <div class="col-md-6">                        <asp:Button ID="btnSignup" runat="server" Text="Signup" class="btn btn-primary btn-block" ValidationGroup="signup" OnClick="btnSignup_Click" />                    </div>                    <div class="col-md-3">                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-9">                        <div class="alert-info ">                            <div id="divMessage" runat="server" class="divMessage" validationgroup="signup">                                <asp:Literal runat="server" ID="ltMessage"></asp:Literal>                            </div>                        </div>                    </div>                    <div class="col-md-3">                    </div>                </div>            </div>            <div class="col-md-3">            </div>        </div>    </div>
</asp:Content>

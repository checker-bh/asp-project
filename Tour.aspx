<%@ Page Title="Tour" Language="C#" MasterPageFile="~/MainMaster.Master" AutoEventWireup="true" CodeBehind="Tour.aspx.cs" Inherits="BAHRAIN_TOURS.Tour1" %>
<asp:Content ID="Content1" ContentPlaceHolderID="head" runat="server">
</asp:Content>
<asp:Content ID="Content2" ContentPlaceHolderID="ContentPlaceHolder1" runat="server">
        <div class="container">        <h1 class="text-center" style="color: green">Booking Page</h1>        <asp:Label ID="lblSession" runat="server" Text=""></asp:Label>        <div class="row">            <div class="col-md-8">                <div class="row txtrow">                    <div class="col-md-3">                        Id                    </div>                    <div class="col-md-9">                        <asp:TextBox class="resizedTextbox" ID="txtID" runat="server" BackColor="#efefef" ReadOnly="true"></asp:TextBox>                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                        Tour Name                    </div>                    <div class="col-md-9">                        <asp:TextBox BackColor="#efefef" ReadOnly="true" class="resizedTextbox" ID="txtTourName" runat="server"></asp:TextBox>                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                        <lable>Package Type :</lable>                    </div>                    <div class="col-md-9">                        <asp:DropDownList class="resizedTextbox" ID="ddlPackageType" runat="server">                            <asp:ListItem Text="Please choose package" Value=""></asp:ListItem>                            <asp:ListItem Text="Basic" Value="Basic"></asp:ListItem>                            <asp:ListItem Text="Gold" Value="Gold"></asp:ListItem>                            <asp:ListItem Text="Premium" Value="Premium"></asp:ListItem>                        </asp:DropDownList>                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                        <lable>Payment Type :</lable>                    </div>                    <div class="col-md-9">                        <asp:DropDownList class="resizedTextbox" ID="ddlPaymentType" runat="server">                            <asp:ListItem Text="Please choose Payment Type" Value=""></asp:ListItem>                            <asp:ListItem Text="Cash" Value="Cash"></asp:ListItem>                            <asp:ListItem Text="Card" Value="Card"></asp:ListItem>                        </asp:DropDownList>                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                        <lable>Decription:</lable>                    </div>                    <div class="col-md-9">                        <textarea class="resizedTextbox" id="txtDesc" cols="20" rows="4" runat="server"></textarea>                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                        <lable>Start Date:</lable>                    </div>                    <div class="col-md-9">                        <input class="resizedTextbox" id="StartDate" type="date" runat="server" />                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                        <lable>End Date:</lable>                    </div>                    <div class="col-md-9">                        <input class="resizedTextbox" id="EndDate" type="date" runat="server" />                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                        <lable>Price:</lable>                    </div>                    <div class="col-md-9">                        <asp:TextBox class="resizedTextbox" ReadOnly="true" BackColor="#efefef" ID="txtPrice" runat="server" Text=""></asp:TextBox>                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                         <lable>Company ID :</lable>                    </div>                    <div class="col-md-9">                        <asp:TextBox class="resizedTextbox" type="" ID="txtCompanyID" runat="server" Text=""></asp:TextBox>                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                         <lable>GuideID :</lable>                    </div>                    <div class="col-md-9">                        <asp:TextBox class="resizedTextbox" type="" ID="txtGuideID" runat="server" Text=""></asp:TextBox>                    </div>                </div>                <div class="row txtrow">                    <div class="col-md-3">                    </div>                    <div class="col-md-9">                        <asp:TextBox class="resizedTextbox" BackColor="#efefef" ID="txtUsername" runat="server" Text="" type="hidden" ReadOnly="true"></asp:TextBox>                    </div>                </div>                <div calss="row txtrow">                    <div class="col-md-3">                    </div>                    <div class="col-md-9" style="align-items: flex-start;">                        <asp:Button ID="btnInsert" runat="server" Text="Insert" class="btn btn-success" OnClick="btnInsert_Click" />                        <asp:Button ID="btnDelete" runat="server" Text="Delete" class="btn btn-danger" OnClick="btnDelete_Click" />                        <asp:Button ID="btnUpdate" runat="server" Text="Update" class="btn btn-warning" OnClick="btnUpdate_Click" />                        <asp:Button ID="btnView" runat="server" Text="View" class="btn btn-info" OnClick="btnView_Click" />                    </div>                </div>            </div>            <div class="col-md-4">            </div>        </div>    </div>    <br />    <div class="container">        <asp:GridView ID="grviewTour" runat="server" BackColor="White" BorderColor="#999999" BorderStyle="Solid" BorderWidth="1px" CellPadding="3" ForeColor="Black" GridLines="Vertical" AutoGenerateSelectButton="True" OnSelectedIndexChanged="grviewTour_SelectedIndexChanged">            <AlternatingRowStyle BackColor="#CCCCCC" />            <FooterStyle BackColor="#CCCCCC" />            <HeaderStyle BackColor="Black" Font-Bold="True" ForeColor="White" />            <PagerStyle BackColor="#999999" ForeColor="Black" HorizontalAlign="Center" />            <SelectedRowStyle BackColor="#000099" Font-Bold="True" ForeColor="White" />            <SortedAscendingCellStyle BackColor="#F1F1F1" />            <SortedAscendingHeaderStyle BackColor="#808080" />            <SortedDescendingCellStyle BackColor="#CAC9C9" />            <SortedDescendingHeaderStyle BackColor="#383838" />        </asp:GridView>    </div>
</asp:Content>
